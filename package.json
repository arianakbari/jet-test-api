{
  "name": "jet-test-api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app/main.js",
    "start:dev": "pnpm build && ts-node src/app/main.ts --watch",
    "start:debug": "pnpm start:dev --debug",
    "build": "tsc --project tsconfig.build.json",
    "live": "liara deploy --app=\"jet-test-api\" --no-app-logs --platform=node",
    "lint": "eslint --ignore-path ./.gitignore .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "node --no-compilation-cache --expose-gc ./node_modules/jest/bin/jest -c jest.config.ts",
    "test:coverage": "pnpm test -- --coverage",
    "test:watch": "pnpm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/jest/bin/jest --runInBand",
    "env:copy": "cp .env.example .env"
  },
  "private": true,
  "imports": {
    "#src/*": "./dist/*",
    "#jest": "./test/jest/index.ts"
  },
  "engines": {
    "node": ">= 18.18.1",
    "pnpm": ">= 8.9.0"
  },
  "packageManager": "pnpm@8.9.0",
  "dependencies": {
    "body-parser": "1.20.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "class-validator-extended": "3.2.0",
    "cookie-parser": "1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "inversify": "6.0.2",
    "inversify-express-utils": "6.4.6",
    "ioredis": "5.3.2",
    "ip": "1.1.8",
    "joi": "17.12.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "pg": "8.10.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "pusher": "5.1.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17",
    "uuid": "9.0.1",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@liara/cli": "5",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.7",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "junit-report-merger": "6.0.2",
    "prettier": "2.6.2",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "uuid": "9.0.1"
  }
}
